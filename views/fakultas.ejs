<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Fakultas Data</title>
    <link rel="stylesheet" href="/path/to/your/styles.css" />
  </head>
  <body>
    <h1>Data Fakultas</h1>
    <table border="1">
      <thead>
        <tr>
          <th>Fakultas</th>
          <th>Singkatan</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(function(item) { %>
        <tr>
          <td><%= item.fakultas %></td>
          <td><%= item.singkatan %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
